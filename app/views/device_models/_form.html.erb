<%= semantic_form_for @device_model do |f| %>
<%= f.inputs do %>
  <%= f.input :title %>
  <%= f.input :device_brand %>
  <%= f.input :device_type %>
  <%= f.input :rack_units %>
  <%= f.input :ethernet_interfaces %>
  <%= f.input :fiber_interfaces %>
  <%= f.input :stock_unit %>
<% end %>

<%= f.actions do %>
<%= f.action :submit, :as => :input %>
<% end %>
<% end %>