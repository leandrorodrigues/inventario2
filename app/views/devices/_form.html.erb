<%= semantic_form_for @device do |f| %>
<%= f.inputs do %>
  <%= f.input :title %>
  <%= f.input :item %>
  <%= f.input :device_model %>
  <%= f.input :ip %>
  <%= f.input :host %>
  <%= f.input :description %>
  <%= f.input :unknown %>
<% end %>

<%= f.actions do %>
<%= f.action :submit, :as => :input %>
<% end %>
<% end %>