<%= semantic_form_for @container do |f| %>
<%= f.inputs do %>
  <%= f.input :title %>
  <%= f.input :place %>
  <%= f.input :item %>
  <%= f.input :container_type %>
  <%= f.input :reverse %>
  <%= f.input :container %>
<% end %>

<%= f.actions do %>
<%= f.action :submit, :as => :input %>
<% end %>
<% end %>